<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/anal_goal_weekly_month"
        android:layout_width="match_parent"
        android:layout_height="38.5dp"
        android:gravity="center"
        android:textColor="@color/sweet_black"
        android:textSize="14dp" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_marginLeft="34dp"
        android:layout_marginRight="34dp"
        android:background="@drawable/graph_5_img_topline" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="157dp"
        android:layout_marginLeft="34dp"
        android:layout_marginRight="34dp"
        android:orientation="horizontal"
        android:paddingLeft="6dp"
        android:paddingRight="6dp" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="54"
            android:gravity="bottom|center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/anal_goal_weekly_week_1_text"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="119"
                android:gravity="bottom|center_horizontal"
                android:text="0"
                android:textColor="@color/sweet_black"
                android:textSize="11dp" />

            <ImageView
                android:id="@+id/anal_goal_weekly_week_1_bar"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0"
                android:background="@drawable/home_shape_analysis_bar" />
        </LinearLayout>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="56" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="54"
            android:gravity="bottom|center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/anal_goal_weekly_week_2_text"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="119"
                android:gravity="bottom|center_horizontal"
                android:text="0"
                android:textColor="@color/sweet_black"
                android:textSize="11dp" />

            <ImageView
                android:id="@+id/anal_goal_weekly_week_2_bar"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0"
                android:background="@drawable/home_shape_analysis_bar" />
        </LinearLayout>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="56" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="54"
            android:gravity="bottom|center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/anal_goal_weekly_week_3_text"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="119"
                android:gravity="bottom|center_horizontal"
                android:text="0"
                android:textColor="@color/sweet_black"
                android:textSize="11dp" />

            <ImageView
                android:id="@+id/anal_goal_weekly_week_3_bar"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0"
                android:background="@drawable/home_shape_analysis_bar" />
        </LinearLayout>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="56" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="54"
            android:gravity="bottom|center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/anal_goal_weekly_week_4_text"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="119"
                android:gravity="bottom|center_horizontal"
                android:text="0"
                android:textColor="@color/sweet_black"
                android:textSize="11dp" />

            <ImageView
                android:id="@+id/anal_goal_weekly_week_4_bar"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0"
                android:background="@drawable/home_shape_analysis_bar" />
        </LinearLayout>

        <View
            android:id="@+id/anal_goal_weekly_week_5_empty"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="56"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/anal_goal_weekly_week_5_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="54"
            android:gravity="bottom|center_horizontal"
            android:orientation="vertical"
            android:visibility="gone" >

            <TextView
                android:id="@+id/anal_goal_weekly_week_5_text"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="119"
                android:gravity="bottom|center_horizontal"
                android:text="0"
                android:textColor="@color/sweet_black"
                android:textSize="11dp" />

            <ImageView
                android:id="@+id/anal_goal_weekly_week_5_bar"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0"
                android:background="@drawable/home_shape_analysis_bar" />
        </LinearLayout>

        <View
            android:id="@+id/anal_goal_weekly_week_6_empty"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="56"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/anal_goal_weekly_week_6_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="54"
            android:gravity="bottom|center_horizontal"
            android:orientation="vertical"
            android:visibility="gone" >

            <TextView
                android:id="@+id/anal_goal_weekly_week_6_text"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="119"
                android:gravity="bottom|center_horizontal"
                android:text="0"
                android:textColor="@color/sweet_black"
                android:textSize="11dp" />

            <ImageView
                android:id="@+id/anal_goal_weekly_week_6_bar"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0"
                android:background="@drawable/home_shape_analysis_bar" />
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:layout_marginLeft="34dp"
        android:layout_marginRight="34dp"
        android:background="@drawable/graph_5_img_underline" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginLeft="34dp"
        android:layout_marginRight="34dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/anal_goal_weekly_week_1_period"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="bottom|left"
            android:paddingLeft="5dp"
            android:textColor="@color/sweet_black"
            android:textSize="8dp" />

        <TextView
            android:id="@+id/anal_goal_weekly_week_2_period"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal|bottom"
            android:textColor="@color/sweet_black"
            android:textSize="8dp" />

        <TextView
            android:id="@+id/anal_goal_weekly_week_3_period"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal|bottom"
            android:textColor="@color/sweet_black"
            android:textSize="8dp" />

        <TextView
            android:id="@+id/anal_goal_weekly_week_4_period"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal|bottom"
            android:textColor="@color/sweet_black"
            android:textSize="8dp" />

        <TextView
            android:id="@+id/anal_goal_weekly_week_5_period"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal|bottom"
            android:textColor="@color/sweet_black"
            android:textSize="8dp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/anal_goal_weekly_week_6_period"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="bottom|right"
            android:textColor="@color/sweet_black"
            android:textSize="8dp"
            android:visibility="gone" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="23.5dp"
        android:layout_marginRight="29dp"
        android:gravity="right|center_vertical"
        android:text="한 달간의 목표 도달 기록을 보여줍니다."
        android:textColor="@color/sweet_blue"
        android:textSize="10dp" />

</LinearLayout>